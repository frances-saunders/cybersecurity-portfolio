{
  // Require MFA specifically for Azure portal sign-ins
  "id": "ca-require-mfa-portal",
  "displayName": "Require MFA for Azure Portal Access",
  "state": "enabled",
  "conditions": {
    "applications": {
      "includeApplications": ["797f4846-ba00-4fd7-ba43-dac1f8f63013"] // Azure Portal App ID
    },
    "users": {
      "includeUsers": ["All"]
    }
  },
  "grantControls": {
    "operator": "OR",
    "builtInControls": ["mfa"]
  }
}
